@model IEnumerable<VTel.Models.SANPHAM>

@{
    ViewBag.Title = "Trang Chủ";
    ViewBag.Current = "TrangChu";

}

<link href="~/Content/css/Product/products-main.css" rel="stylesheet" />
<link href="~/Content/css/Product/products-list.css" rel="stylesheet" />
<link href="~/Content/css/IndexHome/home.css" rel="stylesheet" />

<div id="myCarousel" class="carousel slide" data-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner">
        <div class="item active">
            <img src="https://lh3.googleusercontent.com/wqxCVamqVYdj3HT8qQ6kK8COPD97KLkH17Bdf-q9enMOzvoSg9xvw7EiNc3AXsk6pB2mIc9e2_lLdAUiqRWZGdroqybJhbo44w=w1920-rw" alt="VTel1">
        </div>

        <div class="item">
            <img src="https://lh3.googleusercontent.com/uhFG93y3uzRI1Hb-NomRB4wgsVRytHcYyjCfYEs4Cievyl7sJRvvPQ-Z1OGkPPABkI4OlBf5VNkchz_4Un522DqQTXovjcRFtg=w1920-rw" alt="VTel2">
        </div>

        <div class="item">
            <img src="https://lh3.googleusercontent.com/MgUEMZPMSNa92CZnC50UZk63653FkpJY0XWSS2zOubhq2-Q2yURizOh4cSptPrupaWbFB6YHBIgB3t4XGmZIQHY3-cNxEpxxCQ=w1920-rw" alt="VTel3">
        </div>
    </div>

    <!-- Left and right controls -->
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

<div class="container">
    <div>
        <div>
            @*tesst*@
            <a class="made-with" href="http://flkt-crnpio.github.io/pitaya/">made with<br>pitaya</a>

            <h4 class="text-center"></h4>
            <div class="tabs" style="padding: 24px 0px;">
                <div class="tab-2">
                    <label for="tab2-1">Sản Phẩm Mới</label>
                    <input id="tab2-1" name="tabs-two" type="radio" checked="checked">
                    <div>
                        <div>
                            @*sản phẩm mới*@
                            <div class="row">
                                @foreach (var pro in Model)
                                {
                                    <div class="col-md-4">
                                        <a href="@Url.Action("Details", "Products", new { id = pro.PRODUCTID })" class="products-link">
                                            <div class="products-box container-fluid p-0">
                                                <div class="product-box-heading">
                                                    <img src="@Url.Content(pro.HINHANHSP)" alt="@Html.DisplayFor(model => pro.TENSP)" class="product-img img-fluid">
                                                </div>
                                                <div class="product-box-body p-2">
                                                    <h5 class="product-title m-0">
                                                        <a href="./detail-product.html">@Html.DisplayFor(model => pro.TENSP)</a>
                                                    </h5>
                                                    <div class="row product-teacher-section p-0">
                                                        <div class="col-6 text-left custom-text-show">
                                                            <p class="product-teacher m-0">@Html.DisplayFor(model => pro.MOTASP)</p>
                                                        </div>

                                                        <div class="col-6 text-right">
                                                            <p class="sale m-0">
                                                                @Convert.ToDecimal(pro.GIASP).ToString("N0")
                                                                ₫
                                                            </p>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-6 text-left m-0">
                                                            <i class="fa fa-star text-warning" aria-hidden="true"></i>
                                                            <i class="fa fa-star text-warning" aria-hidden="true"></i>
                                                            <i class="fa fa-star text-warning" aria-hidden="true"></i>
                                                            <i class="fa fa-star text-warning" aria-hidden="true"></i>
                                                            <i class="fa fa-star text-warning" aria-hidden="true"></i>
                                                            (100)
                                                        </div>

                                                        <div class="col-6 text-right">
                                                            <p class="price m-0">
                                                                @Convert.ToDecimal(pro.GIASP).ToString("N0")
                                                                ₫
                                                            </p>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </a>
                                    </div>
                                }
                                <!--Card info-->
                            </div>
                        </div>
                    </div>
                </div>
                <div class="tab-2">
                    <label for="tab2-2">Nhiều Người Mua</label>
                    <input id="tab2-2" name="tabs-two" type="radio">
                    <div>
                        <div>
                            @*sản phẩm mới*@
                            <div class="row">
                                @foreach (var pro in Model)
                                {
                                    <div class="col-md-4">
                                        <a href="@Url.Action("Details", "Products", new { id = pro.PRODUCTID })" class="products-link">
                                            <div class="products-box container-fluid p-0">
                                                <div class="product-box-heading">
                                                    <img src="@Url.Content(pro.HINHANHSP)" alt="@Html.DisplayFor(model => pro.TENSP)" class="product-img img-fluid">
                                                </div>
                                                <div class="product-box-body p-2">
                                                    <h5 class="product-title m-0">
                                                        <a href="./detail-product.html">@Html.DisplayFor(model => pro.TENSP)</a>
                                                    </h5>
                                                    <div class="row product-teacher-section p-0">
                                                        <div class="col-6 text-left custom-text-show">
                                                            <p class="product-teacher m-0">@Html.DisplayFor(model => pro.MOTASP)</p>
                                                        </div>

                                                        <div class="col-6 text-right">
                                                            <p class="sale m-0">@Html.DisplayFor(model => pro.GIASP)</p>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-6 text-left m-0">
                                                            <i class="fa fa-star text-warning" aria-hidden="true"></i>
                                                            <i class="fa fa-star text-warning" aria-hidden="true"></i>
                                                            <i class="fa fa-star text-warning" aria-hidden="true"></i>
                                                            <i class="fa fa-star text-warning" aria-hidden="true"></i>
                                                            <i class="fa fa-star text-warning" aria-hidden="true"></i>
                                                            (100)
                                                        </div>

                                                        <div class="col-6 text-right">
                                                            <p class="price m-0">
                                                                @Convert.ToDecimal(pro.GIASP).ToString("N0")
                                                                ₫
                                                            </p>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </a>
                                    </div>
                                }
                                <!--Card info-->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            @*end tesst*@
        </div>
    </div>
</div>
<div style="background-image: url(https://lh3.googleusercontent.com/SQ1sFzGsSaUtB52h4L30QNQh2ZprwZMA7h6Xktv7QGUfoJq0rpivHrFiKiDv4gNrBzj3msNg0AnbeXFeU1E0Z0Gkg552vitX=w1920-rw); height: 360px;">

