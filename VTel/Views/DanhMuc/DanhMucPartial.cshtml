@using VTel.Models
@model List<DANHMUC>
<style>
    #div-u {
        padding-top: 30px;
        margin-top: 50px;
        background-color: aquamarine;
    }
</style>

<ul style="display: inline-block; padding: 6px;">
    @{
        foreach (var item in Model)
        {
        <li class="div-cate">
            <img src="https://cdn-icons-png.flaticon.com/512/3039/3039367.png" alt="Ngoại ngữ" width="25">
            @Html.ActionLink(item.TENDANHMUC, "Index", "Products", new { danhmuc = item.MADANHMUC.Trim()},"")
        </li>
        }
    }
</ul>